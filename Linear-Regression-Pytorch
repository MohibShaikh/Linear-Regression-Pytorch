    "def gda(loss,w,b):\n",
    "    with torch.no_grad():\n",
    "        w -= w.grad * 1e-2\n",
    "        b -= b.grad * 1e-2\n",
    "        w.grad.zero_()\n",
    "        b.grad.zero_()"
   ]
